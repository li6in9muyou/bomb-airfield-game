<log4net>
    <root>
        <level value="ALL" />
        <appender-ref ref="console" />
    </root>
    <appender name="console" type="log4net.Appender.ConsoleAppender">
        <layout type="log4net.Layout.PatternLayout">
            <conversionPattern value="%logger [%message] |%level|--|thread:%thread|%stacktrace{3}|%date{HH:mm:ss,fff}|%newline" />
        </layout>
    </appender>
</log4net>